;\n\nconst Restaurant = (props) => {\n\n    const history = useHistory();\n\n    const showOpinions = async (restaurant) => {\n        props.dispatch({ type: SHOW_COMMERCE, payload: restaurant});\n        history.push('/');\n\n\n    }\n\n    useEffect(() => {\n        axios.get(process.env.REACT_APP_API_URL +'/commerces/restaurants')\n            .then(res => {\n                props.dispatch({ type: GET_ALL_RESTAURANTS, payload: res.data });\n            })\n    }, [])\n    \n    return (\n        <div className=\"commerces\">\n            {props.restaurants?.map(restaurant => {\n                return (\n                    <div className=\"cardCommerces\">\n                        <h3>{restaurant.name} - {restaurant.city}</h3>\n                        <img className=\"commerceImage\" src={restaurant.image}></img>\n                        <p>{restaurant.review}</p>\n                        <button className=\"buttons\" onClick={ () => { showOpinions(restaurant) }}>Ver Opiniones</button>\n                    </div>\n                )\n            } )}\n        </div>\n    )\n    \n\n}\nconst mapStateToProps = state => {\n    return {\n        restaurants: state.restaurants,\n        commerce: state.commerce\n    }\n}\nexport default connect(mapStateToProps) (Restaurant);\n","/Users/Javi/Desktop/Proyecto FINAL/Frontend/the-perfect-day-front/src/containers/Photographers/Photographers.jsx",["79","80"],"import React, { useEffect } from 'react';\nimport axios from 'axios';\n\nimport './Photographers.scss';\nimport { GET_ALL_PHOTOGRAPHERS} from '../../redux/types';\nimport { connect } from 'react-redux';\n\nconst Photographers = (props) => {\n\n    useEffect(() => {\n        axios.get(process.env.REACT_APP_API_URL +'/commerces/photographers')\n            .then(res => {\n                props.dispatch({ type: GET_ALL_PHOTOGRAPHERS, payload: res.data });\n            })\n    }, [])\n    \n    return (\n        <div className=\"commerces\">\n            {props.photographers?.map(photographer => {\n                return (\n                    <div className=\"cardCommerces\">\n                        <h3>{photographer.name} - {photographer.city}</h3>\n                        <img className=\"commerceImage\" src={photographer.image}></img>\n                        <p>{photographer.review}</p>\n                    </div>\n                )\n            } )}\n        </div>\n    )\n    \n\n}\nconst mapStateToProps = state => {\n    return {\n        photographers: state.photographers\n    }\n}\nexport default connect(mapStateToProps) (Photographers);\n","/Users/Javi/Desktop/Proyecto FINAL/Frontend/the-perfect-day-front/src/containers/Florist/Florist.jsx",["81","82"],"import React, { useEffect } from 'react';\nimport axios from 'axios';\nimport { connect } from 'react-redux';\nimport { GET_ALL_FLORIST} from '../../redux/types';\n\n\n\nimport './Florist.scss';\n\nconst Florist = (props) => {\n\n    useEffect(() => {\n        axios.get(process.env.REACT_APP_API_URL +'/commerces/florist')\n            .then(res => {\n                props.dispatch({ type: GET_ALL_FLORIST, payload: res.data });\n            })\n    }, [])\n\n    return (\n        <div className=\"commerces\">\n            {props.florist?.map(florist => {\n                return (\n                    <div className=\"cardCommerces\">\n                        <h3>{florist.name} - {florist.city}</h3>\n                        <img className=\"commerceImage\" src={florist.image}></img>\n                        <p>{florist.review}</p>\n                    </div>\n                )\n            } )}\n        </div>\n    )\n\n}\n\nconst mapStateToProps = state => {\n    return {\n        florist: state.florist\n    }\n}\n\nexport default connect(mapStateToProps) (Florist);","/Users/Javi/Desktop/Proyecto FINAL/Frontend/the-perfect-day-front/src/containers/Beauty/Beauty.jsx",["83","84"],"import React, { useEffect } from 'react';\nimport axios from 'axios';\nimport { GET_ALL_BEAUTY} f